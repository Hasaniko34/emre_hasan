{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "app.py",
    "MATPLOTLIB_BACKEND": "Agg"
  },
  "functions": {
    "index.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 